service EasyROP
{
        disable = no
        type = UNLISTED
        wait = no
        server = /home/EasyROP/run.sh
        socket_type = stream
        protocol = tcp
        user = EasyROP
        port = 4444
        flags = REUSE
        per_source = 5
        rlimit_cpu = 3
        nice = 18
}
